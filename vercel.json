{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "methods": ["POST"],
      "dest": "index.py"
    },
    {
      "src": "/notify_ip",
      "methods": ["POST"],
      "dest": "index.py"
    }
  ]
}
